.nav-stacked>li {transition: all ease 0.8s;}
.nav-stacked>li:hover{box-shadow: inset -500px 0 0 0 rgba(185, 182, 182, 0.82);transition: all ease 0.8s;}
.nav>li.user-list>a:hover, .nav>li.user-list>a:focus{background-color: transparent;}
.nav-stacked>li+li{margin:0}

.user-det { margin: 0px; padding: 0px; }
.user-det li { margin: 0px; padding: 0px; display: block; }
.user-det a.dropdown-toggle { padding: 10px 0 !important; }
.user-det img {border-radius: 50%; height: 30px; margin: 0 9px; width: 30px; }
.user-window {height: 100%;
    border-right: 1px solid #AFAFAF;
    overflow: auto;
}
#user-chat-window{height: 100%;background-color: #e5e5e5;overflow: hidden;}
.chatter-window{height: 428px;
    border: 1px solid #AFAFAF;
    padding: 0;}
.nopadding {
   padding: 0 !important;
   margin: 0 !important;
}
.user-list{border-bottom: 1px solid #AFAFAF;}
.user-list .badge {background-color: #6CC16E;}
